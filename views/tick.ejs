				<div class="form-group">
					<% doc.forEach(function(info){ %>
						<button class="btn btn-lg btn-primary btn-block"
							onclick="location.href='/newtick/<%=info._id %>'">New Ticket</button>
						<% }) %>
				</div>
				<div class="form-group">
					<% doc.forEach(function(det){ %>
						<button class="btn btn-lg btn-primary btn-block"
							onclick="location.href='/existing/<%= det._id %>'">Existing ticket</button>
						<% }) %>
				</div>
				<div class="form-group">
					<% doc.forEach(function(ptid){ %>
						<button class="btn btn-lg btn-primary btn-block"
							onclick="location.href='/profile/<%= ptid._id%>'">Profile</button>
						<% })%>
				</div>
				<div class="form-group">
					<button class="btn btn-lg btn-primary btn-block" onclick="location.href='/help'">Help</button>
				</div>
				<div class="form-group">
					<button class="btn btn-lg btn-primary btn-block" onclick="location.href='/aboutus'">About us</button>
				</div>
			</div>
		</div>



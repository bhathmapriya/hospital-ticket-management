<body>
	<nav class="navbar navbar-default "
		style="min-height:100px;background: linear-gradient(to right,#80d4ff,#00a3cc,#006680);">
		<div class="container-fluid">
			<div class="navbar-header">
				<a class="navbar-brand" href=" ">Generate Ticket</a>
			</div>
			<div class="  collapse navbar-collapse ">

				<ul class="nav navbar-nav navbar-right mr-auto">
					<% doc.forEach(function(det){ %>
						<li><b style="    color: white;
							padding-right: 10px;">
								<%= det.username %>
							</b></li>
						<% for (let img of det.imagee){%>

							<li>
								<img src="<%=img.url %>"
									style="width:46px; height: 46px; border-radius: 50%; overflow:hidden; margin-top:-6px"
									class="img-fluid" onclick="myfunction(src)">
							</li>
							<script>
								function myfunction(x) {
									// location.href = x;
									document.write("<img src=" + x + " style=width:auto,height:auto /0>");
								}
							</script>
							<% } %>
								<li style="font-size: large;"><a href="/profile/<%= det._id %>">Profile</a></li>
								<% }) %>
									<li style="font-size: large;text-align: center;"><a href="/">Logout</a></li>
				</ul>

			</div>
		</div>
	</nav>






	<div class="container">
		<div style="text-align:center">
			<!-- <h2>New Ticket </h2> --!-->

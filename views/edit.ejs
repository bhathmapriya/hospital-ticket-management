<html>
<head>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"
		integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
	<script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"
		integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN"
		crossorigin="anonymous"></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js"
		integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q"
		crossorigin="anonymous"></script>
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"
		integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl"
		crossorigin="anonymous"></script>
</head>
<body>
    <nav class="navbar navbar-default " style=" min-height: 100px;background: linear-gradient(to right,#80d4ff,#00a3cc,#006680);">
		<div class="container-fluid">
			<div class="navbar-header">
               
                <% datas.forEach(function(tic){ %>
                    <a class="navbar-brand" href="/profile/<%= tic._id %>"><span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span></a>
			
                   
				</div>
			<div class=" collapse navbar-collapse ">


                <div class="container">
                    <div style="text-align:center">
            
                        <h2>Edit account</h2>
                    </div>
                    <div style="width:30%; margin: 0 auto;">
                        <form method="POST" action="/profile/<%= tic._id %>?_method=PUT" enctype="multipart/form-data" class="editt">
                            <div class="form-group">
                                <input class="form-control " type="text" name="username" placeholder="User name" value="<%= tic.username %>" >
                            </div>
            
                            <div class="form-group">
                                <input type="file" id="imagee" title="Upload image" name="imagee" value="<%= tic.imagee %>">
                            </div>
            
                            <div class="form-group">
                                <input class="form-contol" type="number" name="mobilenumber" pattern="[0-9]{3}-[0-9]{3}-[0-9]{4}"
                                    placeholder="mobile" value="<%= tic.mobilenumber %>">
                            </div>
            
                            <div class="form-group">
                                <input class="form-control" type="text" name="address" placeholder="Residential address" value="<%= tic.address %>">
                            </div>
            
                            
            
                            <div class="form-group">
                                <button class="btn btn-lg btn-primary btn-block login form-btn sx" type="submit"
                                    onclick="location.href='/profile/<%= tic._id %>'">Save</button>
                            </div>
                            <% }) %>
                        </form>
                    </div>
                </div>


			</div>
		</div>
	</nav>


</body>
</html>
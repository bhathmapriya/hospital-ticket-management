<body>
	<nav class="navbar navbar-default " style=" min-height: 60px;background: linear-gradient(to right,#80d4ff,#00a3cc,#006680);">
		<div class="container-fluid">
			<div class="navbar-header">
                <% data.forEach(function(tic){ %>

                    <a class="navbar-brand" href="/ticketgen/<%= tic._id %>"><span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span></a>
			
                <% }) %>
				</div>
			<div class=" collapse navbar-collapse ">

				

			</div>

    <div class="container">

        <% data.forEach(function(inf){ %>
			<% for(let img of inf.imagee){ %>

				<p><img src="<%=img.url %>"  onclick="myfunction(src)" style="display: block ; margin-left:auto; margin-right:auto; width: 30%;"></p>
				<script>
					function myfunction(x){
						// location.href = x;
						document.write("<img src="+x+">");
					}
				</script>
			<% }%>
            <label style="font-size: large;">Name          :</label><p><%= inf.username %></p> 
            <label style="font-size: large;">Mobile Number :</label><p><%= inf.mobilenumber %></p> 
            <label style="font-size: large;">Address       :</label><p><%= inf.address %></p> 
            <label style="font-size: large;">email         :</label><p><%= inf.email %></p> 
			     
			<button onclick="location.href='/edit/<%= inf._id %>'" >Edit</button>
       <% })%>
	</div>

